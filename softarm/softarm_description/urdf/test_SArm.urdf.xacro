    <joint name="J3" type="revolute">
		<origin xyz="-0.0705 0.2916 0" rpy="0 0 2.3318"/>
        <parent link="Link2"/>
        <child link="Link3"/>
		<limit effort="30" velocity="1.0" lower="-3.14" upper="3.14" />
        <axis xyz="0 0 1"/>
