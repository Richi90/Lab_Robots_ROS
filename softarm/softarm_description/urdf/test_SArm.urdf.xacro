    # Softhand
    <xacro:softhands type="right" 
        version = "v1.0"
        parent = "Link6"
        position ="0 0.05 0"
        orientation ="1.57 0 3.14"
        model = "power">
    </xacro:softhands>

    # Gazebo plugins
    #   qbMove Advanced actuators
    <gazebo>
        <plugin name="advanced_vsa_1" filename="libadvanced_plugin.so">
            <joint>J1</joint>
            <pub_eltau>false</pub_eltau>
            <pub_state>false</pub_state>
            <sub_ext_tau>false</sub_ext_tau>	 
        </plugin>
    </gazebo>
    <gazebo>
        <plugin name="advanced_vsa_2" filename="libadvanced_plugin.so">
            <joint>J2</joint>
            <pub_eltau>false</pub_eltau>
            <pub_state>true</pub_state> 		# required for the gravity compensation
            <sub_ext_tau>true</sub_ext_tau>		# required for the gravity compensation
        </plugin>
    </gazebo>
    <gazebo>
        <plugin name="advanced_vsa_3" filename="libadvanced_plugin.so">
            <joint>J3</joint>
            <pub_eltau>false</pub_eltau>
            <pub_state>true</pub_state> 		# required for the gravity compensation
            <sub_ext_tau>true</sub_ext_tau> 	# required for the gravity compensation
