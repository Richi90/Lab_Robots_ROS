        <collision>
            <origin xyz="0.019 0.089 0" rpy="0 3.14 -1.29"/>
            <geometry>
                <mesh filename="package://softarm_description/meshes/softarm/stl/arm_angular_m.stl" />
            </geometry>
            <material name="qb_black"/>
        </collision>
        <collision>
            <origin xyz="0.017 0.095 0" rpy="-1.57 0 0.2862"/>
            <geometry>
                <mesh filename="package://simplecube_description/meshes/qb_meshes/stl/qb_flat_flange_m.stl" />
