<?xml version="1.0"?>
<robot name="softarm" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find softhands_description)/urdf/softhands.xacro"/>

	<material name="qb_black">
		<color rgba="0.35 0.35 0.35 1.0"/>
	</material>  
	<material name="qb_blue">
		<color rgba="0.0 0.5 0.8 1.0"/>
	</material>
	
    <link name="world">
    </link> 
    <joint name="ground_fixed" type="fixed">
        <origin xyz="0 0 0.05" rpy="0 0 3.14"/> 
        <parent link="world"/>
        <child link="ground"/>
        <axis xyz="0 0 0"/>
    </joint>
    <link name="ground">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 3.14"/>
            <mass value="1.0"/>
            <inertia ixx="0.1" iyy="0.1" izz="0.1" ixy="0" ixz="0" iyz="0"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
            <geometry>
				<mesh filename="package://softarm_description/meshes/softarm/dae/6_dof_arm_base_frame_m.dae" />
            </geometry>	
			<material name="qb_black"/>
        </visual> 
        <collision>
            <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
            <geometry>
                <mesh filename="package://softarm_description/meshes/softarm/stl/6_dof_arm_base_frame_m.stl" />
            </geometry> 
            <material name="qb_black"/>
        </collision> 
    </link>
    
    # Joint n1
    <joint name="J1" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="ground"/>
        <child link="Link1"/>
		<limit effort="30" velocity="1.0" lower="-3.14" upper="3.14" />
        <axis xyz="0 0 1"/>
    </joint>
    
    <link name="Link1"> 
        <inertial>
            <origin xyz="0 0 0.1225" rpy="0 0 0"/>
            <mass value="0.6200"/>
            <inertia ixx="0.001" iyy="0.001" izz="0.001" ixy="0" ixz="0" iyz="0"/>
        </inertial>	
        <visual>
            <origin xyz="0 0 0.0795" rpy="1.57 0 -1.57"/>
