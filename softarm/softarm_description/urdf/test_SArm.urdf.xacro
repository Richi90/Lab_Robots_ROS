        </plugin>
    </gazebo>

    #   Gravity compensation mechanism
    <gazebo>
        <plugin name="gravity_compensation" filename="libsoftarm_gcomp_plugin.so">
            <namespace>softarm</namespace>
            <joint_2>J2</joint_2>
            <joint_3>J3</joint_3>
            <k_2>-39580</k_2>
            <k_3>-35280</k_3>
        </plugin>
    </gazebo>

    <gazebo>
        <self_collide>1</self_collide>
    </gazebo>

</robot>
